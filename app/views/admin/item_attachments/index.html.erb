<p id="notice"><%= notice %></p>

<h1>Admin Item Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Avatar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_item_attachments.each do |admin_item_attachment| %>
      <tr>
        <td><%= admin_item_attachment.item_id %></td>
        <td><%= admin_item_attachment.avatar %></td>
        <td><%= link_to 'Show', admin_item_attachment %></td>
        <td><%= link_to 'Edit', edit_admin_item_attachment_path(admin_item_attachment) %></td>
        <td><%= link_to 'Destroy', admin_item_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Item Attachment', new_admin_item_attachment_path %>
